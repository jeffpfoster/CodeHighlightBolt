    "code",
    "highlighting",
    "css"
  ],
  "require": {
    "bolt/bolt": ">=2.2,<3.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Royall Spence",
      "email": "royall@royall.us",
      "homepage": "http://royall.us"
    },
    {
      "name": "Mespeche"
    }
  ],
  "autoload": {
    "files": [
      "init.php"
    ],
    "psr-4": {
